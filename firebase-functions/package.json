{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase emulators:start",
    "serve:local": "firebase functions:config:set env.src='.env.local' && firebase emulators:start --import='.firestore' --export-on-exit",
    "serve:dev": "firebase functions:config:set env.src='.env.dev' && firebase emulators:start --import='.firestore' --export-on-exit",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "watch-app": "nodemon --watch ../app/client/src --watch ../app/server/src --ext ts,tsx --exec \"npm --prefix ../app/server run build:firebase\""
  },
  "engines": {
    "node": "22"
  },
  "main": "index.js",
  "dependencies": {
    "@google-cloud/storage": "^7.11.2",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "@turf/boolean-point-in-polygon": "^7.0.0",
    "@turf/turf": "^7.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^6.0.1",
    "iarsenic-types": "^2.0.41",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-helmet": "^7.0.2",
    "koa-static": "^5.0.0",
    "module-alias": "^2.2.3",
    "nodemailer": "^6.9.15",
    "typescript": "^5.5.4",
    "uuid4": "^2.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "firebase-functions-test": "^3.1.0",
    "nodemon": "^3.1.10"
  },
  "private": true,
  "version": "1.0.0",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
